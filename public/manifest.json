{
  "manifest_version": 3,
  "name": "My React Extension",
  "version": "1.0",
  "description": "A sample React extension",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "bull.png",
      "48": "bull.png",
      "128": "bull.png"
    }
  },
  "icons": {
    "16": "bull.png",
    "48": "bull.png",
    "128": "bull.png"
  }
}
